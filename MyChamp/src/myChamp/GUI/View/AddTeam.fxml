<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myChamp.GUI.Controller.AddTeamController">
   <children>
      <TableView fx:id="addTeamTable" layoutX="327.0" layoutY="32.0" prefHeight="523.0" prefWidth="253.0" style="-fx-background-color: grey;">
        <columns>
          <TableColumn fx:id="addTeamColTeams" prefWidth="232.0" text="Teams" />
          <TableColumn minWidth="0.0" prefWidth="0.0" text="C2" />
        </columns>
      </TableView>
      <TextField fx:id="addTeamName" layoutX="30.0" layoutY="232.0" prefHeight="45.0" prefWidth="283.0" promptText="Name..." style="-fx-background-color: grey;">
