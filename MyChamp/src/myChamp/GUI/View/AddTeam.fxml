            <Font size="24.0" />
         </font>
      </TextField>
      <Label layoutX="52.0" layoutY="171.0" text="The name of the team:" textFill="#01ff0e">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <Button fx:id="addTeamButton" layoutX="117.0" layoutY="314.0" mnemonicParsing="false" onAction="#pressedAddButton" style="-fx-background-color: none; -fx-border-color: green;" text="Add&gt;&gt;" textFill="#0fff07">
         <font>
            <Font name="System Bold Italic" size="24.0" />
